{
	"name": "jacebot",
	"version": "6.3.5",
	"description": "A multipurpose Discord bot",
	"main": "dist/index.js",
	"scripts": {
		"start": "node dist/index.js",
		"test": "node --env-file=.env.local dist/index.js",
		"build": "tsc && tsc-alias",
		"build:watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")"
	},
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/KRKleinberg/JaceBot.git"
	},
	"author": "Kevin Kleinberg",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/KRKleinberg/JaceBot/issues"
	},
	"homepage": "https://github.com/KRKleinberg/JaceBot#readme",
	"devDependencies": {
		"@types/node": "^20.11.5",
		"@typescript-eslint/eslint-plugin": "^6.19.0",
		"concurrently": "^8.2.2",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-config-standard-with-typescript": "^43.0.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-n": "^16.6.2",
		"eslint-plugin-promise": "^6.1.1",
		"prettier": "^3.2.4",
		"tsc-alias": "^1.8.8",
		"typescript": "^5.3.3"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.495.0",
		"@aws-sdk/lib-dynamodb": "^3.494.0",
		"@discord-player/extractor": "^4.4.5",
		"@discordjs/opus": "^0.9.0",
		"@distube/ytdl-core": "^4.13.3",
		"@supercharge/strings": "^2.0.0",
		"discord-player": "^6.6.7",
		"discord.js": "^14.14.1",
		"globby": "^14.0.0"
	}
