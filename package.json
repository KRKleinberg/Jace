{
	"name": "jace",
	"version": "6.11.2",
	"description": "A multipurpose Discord music app written in TypeScript.",
	"main": "dist/index.js",
	"scripts": {
		"start": "node dist/index.js",
		"test": "node --env-file=.env.local dist/index.js",
		"build": "tsc && tsc-alias",
		"build:watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")"
	},
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/KRKleinberg/Jace.git"
	},
	"author": "Kevin Kleinberg",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/KRKleinberg/Jace/issues"
	},
	"homepage": "https://github.com/KRKleinberg/Jace#readme",
	"devDependencies": {
		"@types/node": "^22.5.4",
		"concurrently": "^9.0.1",
		"eslint": "^8.57.0",
		"eslint-config-love": "^64.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.30.0",
		"eslint-plugin-n": "^17.7.0",
		"eslint-plugin-promise": "^7.1.0",
		"prettier": "^3.3.3"
	},
	"dependencies": {
		"@discord-player/extractor": "^4.5.1",
		"@discordjs/opus": "^0.9.0",
		"@supercharge/strings": "^2.0.0",
		"discord-player": "^6.7.1",
		"discord-player-youtubei": "^1.3.1",
		"discord.js": "^14.16.2",
		"globby": "^14.0.2",
		"mongodb": "^6.9.0",
		"tsc-alias": "^1.8.10",
		"typescript": "^5.6.2"
	}
}
