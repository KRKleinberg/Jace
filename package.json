{
	"name": "jace",
	"version": "6.10.2",
	"description": "A multipurpose Discord music app written in TypeScript.",
	"main": "dist/index.js",
	"scripts": {
		"start": "node dist/index.js",
		"test": "node --env-file=.env.local dist/index.js",
		"build": "tsc && tsc-alias",
		"build:watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")"
	},
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/KRKleinberg/Jace.git"
	},
	"author": "Kevin Kleinberg",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/KRKleinberg/Jace/issues"
	},
	"homepage": "https://github.com/KRKleinberg/Jace#readme",
	"devDependencies": {
		"@types/node": "^20.14.9",
		"concurrently": "^8.2.2",
		"eslint": "^8.57.0",
		"eslint-config-love": "^53.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-n": "^17.7.0",
		"eslint-plugin-promise": "^6.2.0",
		"prettier": "^3.3.2"
	},
	"dependencies": {
		"@discord-player/extractor": "^4.4.7",
		"@discordjs/opus": "^0.9.0",
		"@distube/ytdl-core": "^4.13.4",
		"@supercharge/strings": "^2.0.0",
		"discord-player": "^6.6.10",
		"discord.js": "^14.15.3",
		"globby": "^14.0.1",
		"mongodb": "^6.8.0",
		"tsc-alias": "^1.8.10",
		"typescript": "^5.5.3"
	}
}
