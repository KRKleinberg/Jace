{
	"name": "jace",
	"version": "9.1.0",
	"description": "A multipurpose Discord music app written in TypeScript.",
	"main": "dist/index.js",
	"scripts": {
		"start": "node --import=tsx --openssl-legacy-provider src/index.ts",
		"test": "node --import=tsx --openssl-legacy-provider --env-file=.env.local src/index.ts"
	},
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/KRKleinberg/Jace.git"
	},
	"author": "Kevin Kleinberg",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/KRKleinberg/Jace/issues"
	},
	"homepage": "https://github.com/KRKleinberg/Jace#readme",
	"imports": {
		"#utils/*": "./src/utils/*.ts",
		"#utils/app": "./src/utils/app/index.ts",
		"#utils/data": "./src/utils/data/index.ts",
		"#utils/player": "./src/utils/player/index.ts"
	},
	"devDependencies": {
		"@eslint/js": "^9.33.0",
		"@types/node": "^24.3.0",
		"eslint": "^9.33.0",
		"ffmpeg-static": "^5.2.0",
		"globals": "^16.3.0",
		"prettier": "^3.6.2",
		"typescript-eslint": "^8.40.0"
	},
	"dependencies": {
		"discord-player": "^7.1.0",
		"discord-player-applemusic": "^1.0.2",
		"discord-player-deezer": "^2.5.0",
		"discord-player-soundcloud": "^1.0.7",
		"discord.js": "^14.21.0",
		"mongodb": "^6.18.0",
		"otpauth": "^9.4.1"
