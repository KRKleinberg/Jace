{
	"name": "jace",
	"version": "6.12.1",
	"description": "A multipurpose Discord music app written in TypeScript.",
	"main": "dist/index.js",
	"scripts": {
		"start": "node dist/index.js",
		"test": "node --env-file=.env.local dist/index.js",
		"build": "tsc && tsc-alias",
		"build:watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")"
	},
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/KRKleinberg/Jace.git"
	},
	"author": "Kevin Kleinberg",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/KRKleinberg/Jace/issues"
	},
	"homepage": "https://github.com/KRKleinberg/Jace#readme",
	"devDependencies": {
		"@types/node": "^22.10.1",
		"concurrently": "^9.1.0",
		"prettier": "^3.4.1"
	},
	"dependencies": {
		"@discord-player/extractor": "^4.5.1",
		"@supercharge/strings": "^2.0.0",
		"discord-player": "^6.7.1",
		"discord-player-youtubei": "^1.3.5",
		"discord.js": "^14.16.3",
		"globby": "^14.0.2",
		"mediaplex": "^1.0.0",
		"mongodb": "^6.11.0",
		"tsc-alias": "^1.8.10",
		"typescript": "^5.7.2"
	}
}
