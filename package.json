{
	"name": "jace",
	"version": "7.0.0-dev.0",
	"description": "A multipurpose Discord music app written in TypeScript.",
	"main": "dist/index.js",
	"scripts": {
		"start": "node dist/index.js",
		"test": "node --env-file=.env.local dist/index.js",
		"build": "tsc && tsc-alias",
		"build:watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")"
	},
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/KRKleinberg/Jace.git"
	},
	"author": "Kevin Kleinberg",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/KRKleinberg/Jace/issues"
	},
	"homepage": "https://github.com/KRKleinberg/Jace#readme",
	"devDependencies": {
		"@types/node": "^22.9.1",
		"concurrently": "^9.1.0",
		"eslint": "^8.57.0",
		"eslint-config-love": "^84.1.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-n": "^17.7.0",
		"eslint-plugin-promise": "^7.1.0",
		"prettier": "^3.3.3"
	},
	"dependencies": {
		"@discord-player/extractor": "^7.0.0-dev.2",
		"@supercharge/strings": "^2.0.0",
		"discord-api-types": "^0.37.107",
		"discord-player": "^7.0.0-dev.2",
		"discord-player-youtubei": "^1.3.0-alpha.1",
		"discord.js": "^14.16.3",
		"globby": "^14.0.2",
		"mediaplex": "^0.0.9",
		"mongodb": "^6.11.0",
		"prism-media": "^1.3.5",
		"tsc-alias": "^1.8.10",
		"typescript": "^5.7.2"
	}
}
